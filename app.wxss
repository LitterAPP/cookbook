page{
  background: #f2f2f2;
}
.click-hover{
  background: #F3F3F7;
}

.fovar image{
  height: 20px;
  width: 20px;
}

.fovar{
  display: flex;
  flex-direction: row;
  align-items: center;
  align-content: center;
  height: 25px;
}
.fovar text{
  font-size: 12px;
  color: #949491;
}
.tag{
  display: flex;
  flex-direction: row;
  overflow:  hidden; 
  text-overflow:  ellipsis;
  white-space: nowrap;  
} 

.tag-text{
  font-size: 12px;
  color: #949491;
  margin:0px 3px;   
}
.name-text{
  font-size: 15px;
  color: #2c2c2c; 
}

.nav-title-text{  
  color: #515151;
  font-size: 16px; 
}

.right-reflush-text{
  font-size:12px;
  color:#949491;
}

.delete{
  background: red;
  font-size: 13px;  
  padding: 5px;
}

.join{
  background: #FF8000;
  font-size: 13px;  
  padding: 5px;
}

.cancel{
  background: #fff;
  font-size: 13px;  
  padding: 5px; 
}

.blank{
  height: 50px;
}

.fix-bottom{
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0px;
  height: 40px;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  background: #fff;
  border-top:1px solid #FF8000; 
  z-index: 999;
}
.wrap-button{
  height: 30px;
  width: 80px;
  display: flex;
  flex-direction: column;
  align-content: center;
  align-items: center;
  margin: 10px 20px;
}
.wrap-button image{
  height: 20px;
  width: 20px;
  position: absolute;
  top: 0px;
}
.wrap-button text{
  font-size: 12px;
  color:#949491;
  position: absolute;
  top: 20px;
}
.wrap-button button{
  height: 120px;
  width: 120px;
   margin-left: 0px;
  margin-right: 0px;
  padding: 0px; 
  position: absolute;
  top: 0px;
  opacity: 0;
  z-index: 99;
}
.wrap-button button::after{
  border: 0px; 
}
.bottom-tips{
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
  justify-content: center;
  margin: 10px 0px;
}